<div class="card-body">
    <h6 class="card-title">{{cards.[3].title}}</h6>
        <p>{{cards.[3].description}}</p>
            <a href={{cards.[3].button.href}} class="btn btn-primary"><span>{{cards.[3].button.text}}</span></a>
</div>
</div>
</div>
</div>
</section>
{{/with}}
<hr>
<section class="col-12">
       <h2 class="col-4">{{winner.title}}</h2>
       </section>
<section class="col-9">
        <table class="table">
            <thead>
                <tr>
                   {{#each winner.table.head}}   
                   <td>{{this}}</td>
                   {{/each}}
                </tr>
            </thead>
            <tbody>
                <tr>
                   <th>{{winner.table.user.[0].place}}</th>
                   <td>{{winner.table.user.[0].name}}</td>
                   <td>{{winner.table.user.[0].surname}}</td>
                   <td>{{winner.table.user.[0].nickname}}</td>
                </tr>
                <tr>
                   <th>{{winner.table.user.[1].place}}</th>
                   <td>{{winner.table.user.[1].name}}</td>
                   <td>{{winner.table.user.[1].surname}}</td>
                   <td>{{winner.table.user.[1].nickname}}</td>
                </tr>
                <tr>
                   <th>{{winner.table.user.[2].place}}</th>
                   <td>{{winner.table.user.[2].name}}</td>
                   <td>{{winner.table.user.[2].surname}}</td>
                   <td>{{winner.table.user.[2].nickname}}</td>
                </tr>
            </tbody>
        </table>
</section>
<hr>
<section class="col-10">
       <h2 class="col-6">{{gallery.title}}</h2>
       </section>
<section class="col-12">
            <div class="row g-2">  
              <div class="col-4">
                <div class="card ">
                   <h5 class="card-header">{{gallery.cards.[0].name}}</h5>
                     <ul class="list-group ">
                        {{#each gallery.cards.[0].items}} 
                            <li class="list-group-item">
                               {{this}}  
                            </li>
                       {{/each}}
                     </ul>
                </div>
             </div>
        <div class="col-4">
             <div class="card ">
                 <h5 class="card-header">{{gallery.cards.[1].name}}</h5>
                     <ul class="list-group ">
                          {{#each gallery.cards.[1].items}} 
                               <li class="list-group-item">
                                  {{this}}  
                                </li>
                          {{/each}}
                    </ul>
             </div>
        </div>
            <div class="col-4">
                <div class="card ">
                   <h5 class="card-header">{{gallery.cards.[2].name}}</h5>
                      <ul class="list-group ">
                         {{#each gallery.cards.[2].items}} 
                             <li class="list-group-item">
                                 {{this}}  
                             </li>
                         {{/each}}
                      </ul>
                </div>
            </div>
</section>
