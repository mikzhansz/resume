<main >
    {{#with navigation}}
    <section class="col-10">
        <div class="row g-4">
            <div class="col-2">
              <a href ={{links.[0].href}} class="btn btn-link text-primary">
              <span>{{links.[0].text}}</span></a>
            </div> 
        <div class="card-header col-2">
               <a href ={{links.[1].href}} class="btn btn-link text-primary">
                   <span>{{links.[1].text}}</span></a>
        </div>  
        <div class="card-header col-2">
                <a href ={{links.[2].href}} class="btn btn-link text-primary">
                    <span>{{links.[2].text}}</span></a>
        </div>
        <div class="card-header col-3">
                <a href ={{button.href}} class="btn btn-primary text-lowercase">
                <span>{{button.text}}</span></a>
        </div>
        </div> 
        </div>
    </section>
   {{/with}}
       <hr>
    {{#with header}}   
<section class="col-12">
        <h1 class="col-6">{{title}}</h1>
            <p class="8">{{description}}</p> 
               <a href ={{button.link}} class="btn btn-primary text-lowercase">
                  <span>{{button.text}}</span></a>
</section>
    {{/with}}
       <hr>
{{#with city}} 
<section class="col-12">
<h2 class="col-2 text-center">{{title}}</h2>
</section>
<section class="col-10">
 <div class="row g-2">  
  <div class="col-6">
   <div class="card">
<img class="card-img-top "
src={{cards.[0].picture.src}} class="card-header" alt={{cards.[0].picture.alt}}>
<div class="card-body">
    <h6 class="card-title">{{cards.[0].title}}</h6>
        <p>{{cards.[0].description}}</p>
            <a href={{cards.[0].button.href}} class="btn btn-primary"><span>{{cards.[0].button.text}}</span></a>
</div>
</div>
</div>
<div class="col-6">
  <div class="card">
<img class="card-img-top "
src={{cards.[1].picture.src}} class="card-header" alt={{cards.[1].picture.alt}}>
<div class="card-body">
    <h6 class="card-title">{{cards.[1].title}}</h6>
        <p>{{cards.[1].description}}</p>
            <a href={{cards.[1].button.href}} class="btn btn-primary"><span>{{cards.[1].button.text}}</span></a>
</div>
</div>
</div>
<div class="col-6">
  <div class="card">
<img class="card-img-top "
src={{cards.[2].picture.src}} class="card-header" alt={{cards.[2].picture.alt}}>
<div class="card-body">
    <h6 class="card-title">{{cards.[2].title}}</h6>
        <p>{{cards.[2].description}}</p>
           <a href={{cards.[2].button.href}} class="btn btn-primary"><span>{{cards.[2].button.text}}</span></a>
</div>
</div>
</div>
<div class="col-6">
