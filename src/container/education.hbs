{{!-- 
Це код container файлу для education сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/education"
--}}

<div>
  <main>
    <section>
        {{!-- <h2>{{title}}"My education"</h2> --}}
        {{> title data="My education" big=true}}
        <ul>
            {{#each main.education }}
              <li>
                 {{name}}
                {{# unless isend}}
                   (ще в процесі)
                {{/unless}}
             </li>
            {{/each}}
        </ul>
    </section>
    <section>
            {{> title data="My certificates" big=false}}
             <ul>
            {{#each main.certificates}}
              <li>
                 {{name}}({{id}})
               </li>
            {{/each}}
         </ul>
        
    </section>
   
 </main>  
   
</div>

